{
  "name": "deku-media-query",
  "version": "1.0.6",
  "description": "Media query component for Deku",
  "license": "MIT",
  "repository": "kevva/deku-media-query",
  "author": {
    "name": "Kevin Martensson",
    "email": "kevinmartensson@gmail.com",
    "url": "github.com/kevva"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "build": "browserify example/index.js -o example/build.js -dv -t [ babelify --presets [ es2015 react ] ]",
    "prepublish": "babel  --presets es2015,react --out-file dist.js index.js",
    "test": "xo && npm run prepublish && ava"
  },
  "main": "dist.js",
  "files": [
    "dist.js"
  ],
  "keywords": [
    "deku",
    "deku-component",
    "dekujs",
    "component",
    "media",
    "query",
    "response"
  ],
  "dependencies": {
    "magic-virtual-element": "^1.0.6",
    "matchmedia": "^0.1.2",
    "object-omit": "^1.0.0"
  },
  "devDependencies": {
    "assert-element": "^0.3.1",
    "ava": "*",
    "babel-cli": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "component-mock": "^0.1.0",
    "deku": "^1.0.0 ",
    "eslint-config-xo-react": "^0.3.0",
    "eslint-plugin-react": "^3.9.0",
    "xo": "*"
  },
  "xo": {
    "envs": [
      "browser",
      "node"
    ],
    "esnext": true,
    "extends": [
      "xo-react"
    ],
    "ignores": [
      "dist.js",
      "example/**"
    ],
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "react/no-unknown-property": 0,
      "react/prop-types": 0,
      "quote-props": 0
    }
  }
}
