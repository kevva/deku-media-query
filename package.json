{
	"name": "deku-media-query",
	"version": "1.1.0",
	"description": "Media query component for Deku",
	"license": "MIT",
	"repository": "kevva/deku-media-query",
	"author": {
		"name": "Kevin MÃ¥rtensson",
		"email": "kevinmartensson@gmail.com",
		"url": "github.com/kevva"
	},
	"engines": {
		"node": ">=0.10.0"
	},
	"scripts": {
		"build": "webpack --module-bind js=babel-loader example/index.js example/build.js",
		"prepublish": "babel --out-file dist.js index.js",
		"test": "xo && ava"
	},
	"main": "dist.js",
	"files": [
		"dist.js"
	],
	"keywords": [
		"deku",
		"deku-component",
		"dekujs",
		"component",
		"media",
		"query",
		"response"
	],
	"dependencies": {
		"deep-equal": "^1.0.1",
		"magic-virtual-element": "^1.0.6",
		"matchmedia": "^0.1.2",
		"object.omit": "^2.0.1"
	},
	"devDependencies": {
		"assert-element": "^0.3.1",
		"ava": "*",
		"babel-cli": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"component-mock": "0.1.0",
		"deku": "^0.5.6",
		"eslint-config-xo-react": "^0.13.0",
		"eslint-plugin-react": "^7.3.0",
		"webpack": "^3.5.5",
		"xo": "*"
	},
	"ava": {
		"babel": "inherit",
		"failWithoutAssertions": false,
		"require": [
			"babel-register"
		]
	},
	"babel": {
		"presets": [
			"es2015",
			"react"
		]
	},
	"xo": {
		"envs": [
			"browser",
			"node"
		],
		"extends": [
			"xo-react"
		],
		"ignores": [
			"dist.js",
			"example/build.js"
		],
		"rules": {
			"jsx-quotes": [
				2,
				"prefer-single"
			],
			"react/no-array-index-key": 1,
			"react/no-unknown-property": 0,
			"react/prop-types": 0,
			"quote-props": 0
		}
	}
}
